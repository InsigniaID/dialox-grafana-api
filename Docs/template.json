            "rawSql": "with cte1 as (\r\n    select * from \"Contact\"\r\n    where \"workspaceId\" = 'workspace-1'\r\n    and channel = 'whatsapp'\r\n),\r\ncte2 as (select *,\r\n     row_number() over (partition by \"workspaceId\" order by \"createdAt\" desc) as rn\r\nfrom \"Message\" where \"senderType\" = 'user' and \"workspaceId\" = 'workspace-1'),\r\ncte3 as (select * from cte2 where rn = 1),\r\ncte4 as (select\r\n       c1.\"phoneNumber\",\r\n       status,\r\n       pricing\r\nfrom \"WhatsappConversation\" wc\r\nleft join cte1 c1 on wc.\"waRecipient\" = c1.\"phoneNumber\"\r\nleft join cte3 c3 on c1.id = c3.\"senderId\"\r\nwhere wc.\"channelId\" in (select \"id\" from \"Channel\" where \"workspaceId\" = 'workspace-1')),\r\n    result as (\r\nselect \"broadcastName\", name, \"phoneNumber\", BD.\"createdAt\", \"templateName\", c4.status, pricing from \"Broadcast\" left join \"BroadcastData\" BD on \"Broadcast\".id = BD.\"broadcastId\"\r\nleft join cte1 c1 using (\"phoneNumber\")\r\nleft join cte4 c4 using (\"phoneNumber\")\r\nwhere \"Broadcast\".\"workspaceId\" = 'workspace-1')\r\nselect \"broadcastName\" as \"Broadcast Name\", name as \"Whatsapp Account\", \"phoneNumber\" as \"Recipients Number\", \"createdAt\" as time, \"templateName\" as \"Message Template\", status as \"Message Last Status\", pricing as \"Source\" from result group by 1,2,3,4,5,6,7",
